    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowActionBarOverlay">true</item>
    </style>
    <style name="EsewaSDK_Button"/>
    <style name="EsewaSDK_Button.Style" parent="esewasdk_match_parent_wrap_content">
        <item name="android:background">@drawable/esewasdk_background_edit_text_green</item>
        <item name="android:textColor">@color/esewasdk_white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">16dp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>
    <style name="EsewaSDK_EditText"/>
    <style name="EsewaSDK_EditText.Box">
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:maxLength">1</item>
        <item name="android:background">@drawable/esewasdk_background_edit_text_light_greyish</item>
        <item name="android:inputType">number</item>
    </style>
    <style name="EsewaSDK_EditText.Style" parent="esewasdk_match_parent_wrap_content">
        <item name="android:background">@drawable/esewasdk_background_edit_text_light_grey</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:padding">16dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/esewasdk_text_dark_300</item>
        <item name="android:imeOptions">actionNext</item>
    </style>
    <style name="EsewaSDK_Switch.Style" parent="esewasdk_match_parent_wrap_content">
        <item name="android:layout_margin">3dp</item>
        <item name="android:theme">@style/SwitchTheme</item>
        <item name="android:text">Remember this eSewa ID</item>
        <item name="android:textColor">@color/esewasdk_text_dark_300</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textOn"/>
        <item name="android:textOff"/>
    </style>
    <style name="EsewaSDK_TextAppearance.Ui.Body1" parent="TextAppearance.MaterialComponents.Body1">
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingMultiplier">1.4</item>
